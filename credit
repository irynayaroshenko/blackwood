#include <stdio.h>
#include <cs50.h>
#include <math.h>

long inp_nmb, dgts, n, o, p, first_d;
int count, a, b, c, m, mm, A, AA, B, C, sum_2;
int main(void)

{
    // get card number from a customer
    inp_nmb = get_long("Input number: ");

    // count digits in a card number
    count = 0;
    n = inp_nmb;
    while (n > 0)
    {
        n = n / 10;
        count++;
    }

    // myltiply by 2 second-to-last digit
    dgts = inp_nmb;
    first_d = inp_nmb;
    A = dgts / pow(10,12);
    AA = dgts / pow(10,13);
    B = dgts / pow(10,14);
    C = dgts / pow(10,15);
    m = A % 10;
    mm = AA % 100;
    o = B % 10;
    p = C % 10;
    dgts = dgts / 10;
    c = first_d % 10;
    if ((count == 13 && m == 4) || (count == 16 && p == 4))
    {
    while (first_d > 0)
    {
        sum_2 = sum_2 + c;
        first_d = first_d / 100;
    }
        
        printf("%i\n", sum_2);
    }
    
    else if (count == 15 && (mm == 34 || mm == 37))   
    {
       printf("AMEX\n");
    }
     
    else if ((count == 16 && p == 5) && (o >= 1 && o <= 5))
    {
        printf("MASTERCARD\n");
    }
 
    else
    {
        printf("INVALID\n");
    }
}  
