#include <stdio.h>
#include <cs50.h>
#include <math.h>

long inp_nmb, dgts, n, o, p;
int count, a, b, m, mm, A, AA, B, C;
int main(void)

{
    // get card number from a customer
    inp_nmb = get_long("Input number: ");

    // count digits in a card number
    count = 0;
    n = inp_nmb;
    while (n > 0)
    {
        n = n / 10;
        count++;
    }

    // myltiply by 2 second-to-last digit
    dgts = inp_nmb;
    A = dgts / pow(10,12);
    AA = dgts / pow(10,13);
    B = dgts / pow(10,14);
    C = dgts / pow(10,15);
    m = A % 10;
    mm = AA % 100;
    o = B % 10;
    p = C % 10;
    dgts = dgts / 10;
    if ((count == 13 && m == 4) || (count == 16 && p == 4))
    {
 //       while (dgts > 0)
 //       {
  //          a = dgts % 10;
  //          b = a * 2;
  //      }
  //      return 0;
   //     printf("%i\n", b);
        printf("VISA\n");
    }
    
    else if (count == 15 && (mm == 34 || mm == 37))   
    {
       printf("AMEX\n");
    }
     
    else if ((count == 16 && p == 5) && (o >= 1 && o <= 5))
    {
        printf("MASTERCARD\n");
    }
 
    else
    {
        printf("INVALID\n");
    }
}  
